with "util";
@WITH_AUNIT@

project Util_Unit is
   Version := "@UTIL_VERSION@";
   Prefix  := "INSTALL";

   for Library_Name use "ada_util_unit";
   for Library_Dir use Prefix & "/lib";
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Prefix & "/share/ada/adainclude/ada-util-unit");
   for Library_ALI_Dir use Prefix & "/lib/ada/adalib/ada-util-unit/";
   for Externally_Built use "true";

end Util_Unit;
