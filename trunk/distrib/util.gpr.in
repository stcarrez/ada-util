@WITH_XML_ADA@

project Util is
   Version := "@UTIL_VERSION@";

   for Library_Name use "adautil";
   for Library_Dir use "INSTALL/lib";
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use ("INSTALL/share/ada/adainclude/util");
