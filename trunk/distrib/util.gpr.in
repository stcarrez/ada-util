@WITH_XML_ADA@

project Util is
   Version := "@UTIL_VERSION@";
   Prefix  := "INSTALL";

   for Library_Name use "ada-util";
   for Library_Dir use Prefix & "/lib";
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Prefix & "/share/ada/adainclude/util", Prefix & "/share/ada/adainclude/util/http");
   for Library_ALI_Dir use Prefix & "/lib/ada/adalib/util/" & Project'Library_Kind;
   for Externally_Built use "true";
end Util;
